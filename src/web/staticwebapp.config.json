{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/*.{css,scss,sass,js,ts,tsx,jsx}", "/*.{png,jpg,jpeg,gif,svg,ico,webp}"]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "application/javascript",
    ".css": "text/css",
    ".html": "text/html"
  },
  "globalHeaders": {
    "Cache-Control": "no-cache"
  }
}